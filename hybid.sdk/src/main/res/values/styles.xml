<?xml version="1.0" encoding="utf-8"?>

<resources>

    <style name="browserProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:indeterminate">false</item>
        <item name="android:progressDrawable">@drawable/browser_progress_bar</item>
        <item name="android:minHeight">2dip</item>
        <item name="android:maxHeight">2dip</item>
    </style>

    <style name="BrowserActivityTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>