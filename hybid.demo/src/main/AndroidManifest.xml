        <activity
            android:name=".ui.activities.hybid.HyBidInterstitialActivity"
            android:label="@string/hybid_interstitial"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activities.mopub.MoPubActivity"
            android:label="@string/mopub"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activities.mopub.MoPubBannerActivity"
            android:label="@string/mopub_banner"
            android:screenOrientation="portrait"
